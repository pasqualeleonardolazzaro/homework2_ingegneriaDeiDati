                enum { ff_pwd, ec_pwd } keyexchangealgorithm;\
\
                struct {\
                  opaque salt<1..2^8-1>;\
                  namedgroup ff_group;\
                  opaque ff_selement<1..2^16-1>;\
                  opaque ff_sscalar<1..2^16-1>;\
